{
  "name": "@digital-roots/vue-slider",
  "version": "0.1.1",
  "description": "2 value range slider",
  "author": "Digital Roots",
  "engines": {
    "node": ">=16"
  },
  "main": "dist/lib/vue-slider.es.js",
  "module": "dist/lib/vue-slider.es.js",
  "browser": "dist/lib/vue-slider.es.js",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check build-only",
    "build:lib": "vite build --config vite.lib.config.ts",
    "preview": "vite preview --port 4173",
    "test:unit": "vitest --environment jsdom",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "stylelint": "stylelint 'src/**/*.{vue,scss}'",
    "stylelint-fix": "stylelint 'src/**/*.{vue,scss}' --fix"
  },
  "dependencies": {
    "sass": "^1.74.1",
    "vue": "^2.7.14",
    "vite": "^5.2.10",
    "@vitejs/plugin-vue2": "^2.3.1"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.1.0",
    "@types/jsdom": "^21.1.6",
    "@types/node": "^20.12.7",
    "@vitejs/plugin-legacy": "^5.3.2",
    "@vitejs/plugin-vue2-jsx": "^1.0.2",
    "@vue/eslint-config-airbnb-with-typescript": "^8.0.0",
    "@vue/eslint-config-typescript": "^11.0.0",
    "@vue/test-utils": "^1.3.0",
    "@vue/tsconfig": "^0.1.3",
    "eslint": "^8.5.0",
    "eslint-plugin-vue": "^9.0.0",
    "jsdom": "^20.0.0",
    "npm-run-all": "^4.1.5",
    "postcss-html": "^1.6.0",
    "stylelint-config-recommended-scss": "^14.0.0",
    "stylelint-config-recommended-vue": "^1.5.0",
    "stylelint-config-standard-scss": "^13.1.0",
    "stylelint-order": "^6.0.4",
    "terser": "^5.14.2",
    "typescript": "~4.7.4",
    "vitest": "^0.18.1",
    "vue-template-compiler": "^2.7.7",
    "vue-tsc": "^0.38.8"
  }
}
